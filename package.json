{
  "name": "react-webpack-bootstrap-boilerplate",
  "description": "Starter kit for React, Bootstrap, Webpack (with Hot Module Replacement) and Babel.",
  "version": "1.0.0",
  "author": "Jordan Rey",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanamr/react-webpack-bootstrap-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/jordanamr/react-webpack-bootstrap-boilerplate/issues"
  },
  "homepage": "https://github.com/jordanamr/react-webpack-bootstrap-boilerplate",
  "scripts": {
    "build": "yarn run clean-dist && cross-env NODE_ENV=production webpack --mode production --config=webpack/webpack.prod.config.js",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{js,jsx}' --quiet",
    "start": "yarn run start:dev",
    "start:dev": "cross-env NODE_ENV=development webpack serve --config=webpack/webpack.dev.config.js"
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "bootstrap": "^4.5.3",
    "jquery": "^3.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.0.0-beta.1",
    "image-webpack-loader": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "postcss": "^8.2.1",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "source-map-loader": "^1.1.3",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-middleware": "^4.0.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.7.2"
  },
  "keywords": [
    "react",
    "webpack",
    "bootstrap",
    "babel",
    "postcss",
    "sass",
    "hmr",
    "boilerplate"
  ]
}
